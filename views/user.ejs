<%- include('header.ejs') %> 
                
                <!-- PAGE CONTENT WRAPPER -->
                <div class="page-content-wrap">

                        <div >
                                <div>
                                    
                                    <!-- START TABLE BLOCK -->
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <div class="panel-title-box">
                                                <h3>Utilisateurs</h3>
                                                <span>Comptes utilisateurs</span>
                                            </div>                                     
                                            
                                        </div>
                                        <div class="panel-body">
                                                                            
                                            <div id="table-container" class="row stacked">
                                                <div class="col-md-4">                                            
                                                    <!-- HERE WE DISPLAY THE MAP-->
                                                    <div class="panel-body panel-body-map">
                                                        <!--<div style="width: 130em; height: 400px; border: 1px solid #AAA;">-->
                                                            <form method="POST"></form>
                                                                <table  class="table datatable" width="1200px">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>Utilisateur</th>
                                                                                    <th>Mot de passe</th>
                                                                                    <th></th>
                                                                                   
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <% users.forEach(function(user){ %>
                                                                                    <tr>
                                                                                        <td id="editable_param_<%= user.id %>"><%= user.username %></td>
                                                                                        <td id="editable_<%= user.id %>">**********</td>
                                                                                        <td>
                                                                                            <button class="btn btn-info" id="edit_<%= user.id %>" onclick="makeEditable('<%= user.id %>')"><span class="glyphicon glyphicon-edit"></span> Editer</button>
                                                                                            <button class="btn btn-success"id="save_<%= user.id %>" onclick="saveEdition('<%= user.id %>')" style="display:none;"><span class="glyphicon glyphicon-save"></span> Enregistrer</button>
                                                                                            <button class="btn btn-warning" id="cancel_<%= user.id %>" onclick="cancelEdition('<%= user.id %>')" style="display:none;"><span class="glyphicon glyphicon-edit"></span> Annuler</button>
                                                                                        </td>
                                                                                    </tr>
                                                                                <% }) %> 
                                                                            </tbody>
                                                                </table>
                                                            </form>

                                                    </div>
                                                </div>
                                                 <!--<div class="col-md-8">
                                                    <div id="dashboard-map-seles" style="width: 100%; height: 200px"></div>
                                                </div>-->
                                            </div>                                    
                                        </div>
                                    </div>
                                    <!-- END TABLE BLOCK -->
                                    
                                </div>
                                
                                
                            </div>
                    
                </div>
                <!-- END PAGE CONTENT WRAPPER -->                                
            </div>            
            <!-- END PAGE CONTENT -->
        </div>
        <!-- END PAGE CONTAINER -->
        
        <script type="text/javascript" src="js/plugins/jquery/jquery-ui.min.js"></script>
        <script type="text/javascript" src="js/plugins/bootstrap/bootstrap.min.js"></script>        
        
        <script type="text/javascript" src="js/plugins.js"></script>        
        <script type="text/javascript" src="js/actions.js"></script>

        <script type="text/javascript" src="js/plugins/datatables/jquery.dataTables.js"></script> 
        <script type='text/javascript' src='maps/script-user.js'></script>      
    </body>
</html>





